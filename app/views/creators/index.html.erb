<% provide(:title, 'All users') %>

<div class="row">

  <div class="container">
    <ul class="nav buttons">
      <li class="active list-button">
         <a data-toggle="tab" id="list-tab-button" href="#points">Points</a>
      </li>
      <li class="show-on-map-button">
        <a data-toggle="tab" id="map-tab-button" href="#paths">Paths</a>
      </li>
    </ul>

    <div class="tab-content">
      <div class="tab-pane fade in active" id="points">
        <%= render 'points' %>
      </div>
      <div class="tab-pane fade" id="paths">
        <%= render 'paths' %>
      </div>
    </div>
  </div>

</div>
